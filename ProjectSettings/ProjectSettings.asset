%YAML 1.1
%TAG !u! tag:unity3d.com,2011:
--- !u!129 &1
PlayerSettings:
  serializedVersion: 12
  companyName: MirrorRide
  productName: MirrorRideGame
  defaultScreenWidth: 1080
  defaultScreenHeight: 2340
  defaultScreenOrientation: 1
  # --- Android graphics API settings ---
  android:
    m_AutoGraphicsAPIs: 0           # disable auto selection
    m_GraphicsAPIs:
      - GLES3                        # prefer OpenGLES3
      - Vulkan                       # fallback to Vulkan
    m_TargetArchitectures: 3        # 1=ARMv7, 2=x86, 4=ARM64; 3 means ARMv7 + ARM64 (or set 8 for ARM64 only)
    m_MinimumApiLevel: 21           # apk minimum (Android 5.0), change to 24 if you want to require newer devices
    m_TargetApiLevel: 33            # Android 13
